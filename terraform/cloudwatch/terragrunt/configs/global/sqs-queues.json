    "customer": "enbd-preprod",
    "team": "DNA",
    "alarms": ["queue_depth", "message_age", "error_rate"],
    "custom_alarms": {}
  },
  "dead-letter-queue": {
    "name": "dead-letter-queue",
    "region": "us-east-1",
    "customer": "enbd-preprod",
    "team": "DNA",
